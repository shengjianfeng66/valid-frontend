{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "i18n-ally.localesPaths": [
    "src/i18n/messages"
  ],
  "i18n-ally.keystyle": "nested",
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/*.json": false
  }
}